{
    "module": "deeper.models.gmvae.gmvae_pure_sampling",
    "model_name": "puresample_1",
    "save_path": "./models/puresample_1",
    "data_paths": {
        "X_train": "./data/mnist/X_train.flat.npy",
        "X_test": "./data/mnist/X_test.flat.npy",
        "y_train": "./data/mnist/y_train.npy",
        "y_test": "./data/mnist/y_test.npy"
    },
    "model_args": {
        "components": 10,
        "input_dimension": 784,
        "embedding_dimensions": [512, 512],
        "latent_dimensions": 64,
        "mixture_embedding_dimensions": [512, 512],
        "mixture_latent_dimensions": 64,
        "embedding_activations": "relu",
        "kind": "binary",
        "learning_rate": 1.0,
        "gradient_clip": null,
        "bn_before": true,
        "bn_after": false,
        "categorical_epsilon": 0.0,
        "reconstruction_epsilon": 0.0,
        "latent_epsilon": 0.0,
        "latent_prior_epsilon": 0.0,
        "z_kl_lambda": 1.0,
        "c_kl_lambda": 1.0,
        "cat_latent_bias_initializer": null,
        "connected_weights": false,
        "optimizer":{
            "class_name": "Adam", 
            "learning_rate": 1e-3
        },
        "categorical_latent_embedding_dropout": 0.2,
        "mixture_latent_mu_embedding_dropout": 0.2,
        "mixture_latent_var_embedding_dropout": 0.2,
        "mixture_posterior_mu_dropout": 0.2,
        "mixture_posterior_var_dropout": 0.2,
        "recon_dropouut": 0.2,
        "latent_fixed_var": null
    },
    "training_args": {
        "num": 100,
        "samples": 1,
        "epochs": 100,
        "iter_train": 1,
        "num_inference": 1000,
        "batch": true, 
        "temperature_function": {
            "class_name": "static",
            "method": "linear",
            "start": 1.0,
            "end": 0.5,
            "alpha": 0.99
        },
        "beta_z_method": {
            "class_name": "constant",
            "const": 1.0
        },
        "beta_y_method": {
            "class_name": "constant",
            "const": 1.0
        },
        "save": null, 
        "save_results": null,
        "tensorboard": null
    }
}